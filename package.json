{
  "name": "unjs-docs",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "bin": {
    "unjs-docs": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "app"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "JITI_ESM_RESOLVE=1 jiti ./src/cli.ts dev playground",
    "dev:build": "JITI_ESM_RESOLVE=1 jiti ./src/cli.ts build playground",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src -w",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint"
  },
  "resolutions": {
    "vue": "3.4.10"
  },
  "dependencies": {
    "@iconify/json": "2.2.169",
    "@nuxt/content": "2.10.0",
    "@nuxt/ui": "2.12.0",
    "@nuxt/ui-pro": "npm:@nuxt/ui-pro-edge@0.7.0-28422581.ddb7ab2",
    "@nuxthq/studio": "1.0.6",
    "@nuxtjs/fontaine": "0.4.1",
    "@nuxtjs/google-fonts": "3.1.3",
    "citty": "0.1.5",
    "consola": "3.2.3",
    "is-buffer": "2.0.5",
    "nuxi": "3.10.0",
    "nuxt": "3.9.1",
    "nuxt-og-image": "2.2.4",
    "pkg-types": "1.0.3",
    "tailwindcss": "3.4.1",
    "vue": "3.4.14",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@types/node": "^20.11.4",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.2.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vue-tsc": "^1.8.27"
  }
}
